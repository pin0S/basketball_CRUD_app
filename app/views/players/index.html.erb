<h1>All Players</h1>

<% @players.each do |player| %>
    <tr>
        <td><%= player.name %></td>
        <td><%= player.school %></td>
        <td><%= player.height %></td>
        <td><%= player.weight %></td>
        <td><%= player.years %></td>
        <td><%= player.age %></td>
        <td><%= player.age %></td>
        <td><%= player.free_agent %></td>
        <td><%= player.position %></td>
        <td><%= player.teams_id %></td>
        <td><%= link_to 'Show', player_path(player) %></td>
    </tr>
    <% end %>
</table>